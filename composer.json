{
    "name": "zf-commons/zfc-user",
    "description": "A highly extensible and pluggable user module for ZF2.",
    "type": "library",
    "keywords": [
        "zf2"
    ],
    "homepage": "https://github.com/ZF-Commons/ZfcUser",
    "authors": [
        {
            "name": "Evan Coury",
            "email": "me@evancoury.com",
            "homepage": "http://blog.evan.pro/"
        },
        {
            "name": "Kyle Spraggs",
            "email": "theman@spiffyjr.me",
            "homepage": "http://www.spiffyjr.me/"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "doctrine/common": "2.*",
        "zendframework/zend-cache": "2.*",
        "zendframework/zend-code": "2.*",
        "zendframework/zend-console": "2.*",
        "zendframework/zend-mvc": "2.*",
        "zendframework/zend-serializer": "2.*",
        "zendframework/zend-version": "2.*"
    },
    "suggest": {
        "zendframework/zend-session": "To use the default authentication adapter."
    },
    "autoload": {
        "psr-0": {
            "ZfcUser": "src/",
            "ZfcUserTest": "test/"
        }
    }
}
